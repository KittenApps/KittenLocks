  "bugs": {
    "url": "https://github.com/KittenApps/KittenLocks/issues"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "homepage": "https://github.com/KittenApps/KittenLocks#readme",
