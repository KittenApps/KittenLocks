{
  "name": "kittenlocks",
  "version": "0.1.0",
  "description": "additional Stats and Charts for your Chaster",
  "scripts": {
    "start": "NODE_ENV=development webpack serve",
    "build": "webpack",
    "build:dev": "NODE_ENV=development webpack",
    "build:prod": "NODE_ENV=production webpack",
    "serve": "NODE_ENV=production webpack && npx http-server -c-1 --proxy=http://127.0.0.1:8080?",
    "stats": "NODE_ENV=production webpack --profile --json > stats.json && npx webpack-bundle-analyzer stats.json public",
    "lint": "eslint . --ignore-path=.gitignore"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KittenApps/KittenLocks.git"
