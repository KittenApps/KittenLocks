{
  "name": "kittenlocks",
  "version": "0.1.0",
  "description": "additional Stats and Charts for your Chaster",
  "scripts": {
    "start": "NODE_ENV=development webpack serve",
    "build": "webpack",
    "build:dev": "NODE_ENV=development webpack",
    "build:prod": "NODE_ENV=production webpack",
    "serve": "NODE_ENV=production webpack && npx http-server --proxy=http://127.0.0.1:8080?",
    "stats": "NODE_ENV=production webpack --profile --json > stats.json && npx webpack-bundle-analyzer stats.json public",
    "lint": "eslint app/**/*.js app/*.js *.js"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KittenApps/KittenLocks.git"
  },
  "author": "Silizia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KittenApps/KittenLocks/issues"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "homepage": "https://github.com/KittenApps/KittenLocks#readme",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/material": "^5.1.1",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.0.0",
    "highcharts": "^9.3.1",
    "html-webpack-plugin": "^5.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.1.2",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.0.2",
    "realm-web": "^1.5.1",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "webpack-dev-server": "^4.5.0"
  }
